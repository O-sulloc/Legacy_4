<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<typeAliases>
		<typeAlias type="com.jh.s1.bankbook.BankBookDTO"
			alias="BankBookDTO" />
		<!-- mapper에서 resulttype같은 곳에 풀패키지 명 적잖아 이거 너무 기니까 여기서 별칭을 만들어주는거임. 풀패키지 
			안적어도되게 별칭으로는 보통 클래스명을 작성함 -->

		<typeAlias type="java.lang.Long" alias="Long" />
	</typeAliases>
</configuration>